<controls:LustdWindow x:Class="Whisper.Apps.Desktop.Windows.Settings.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whisper.Apps.Desktop.Views"
        xmlns:controls="clr-namespace:Whisper.Apps.Desktop.Controls"
        xmlns:assets="clr-namespace:Whisper.Apps.Desktop.Assets"
        mc:Ignorable="d"
        Title="Settings" Height="450" Width="800">

    <controls:LustdWindow.TitleContent>
        <assets:WindowTitle Content="{Binding ElementName=RootWindow, Path=Title}" />
    </controls:LustdWindow.TitleContent>

    <Grid>
        
    </Grid>

</controls:LustdWindow>
